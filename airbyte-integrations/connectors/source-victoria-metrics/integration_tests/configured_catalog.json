{
  "streams" : [
    {
      "stream" : {
        "name" : "victoria_metrics_stream",
        "json_schema" : {
          "$schema" : "http://json-schema.org/draft-07/schema#",
          "type" : "object",
          "properties" : {
            "metric" : {
              "type" : "string"
            },
            "values" : {
              "type" : "array",
              "items" : {
                "type" : "number"
              }
            },
            "timestamps" : {
              "type" : "array",
              "items" : {
                "type" : "number"
              }
            }
          }
        },
        "supported_sync_modes" : [
          "full_refresh"
        ]
      },
      "sync_mode" : "full_refresh",
      "destination_sync_mode" : "append"
    }
  ]
}